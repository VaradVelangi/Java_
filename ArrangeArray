
import java.util.Arrays;

public class ArrangeArray {
    public static void main(String[] args) {
        int a[]={8,7,1,6,5,9};

        int n = a.length;
        Arrays.sort(a);
        for (int i = 0; i < n / 2; i++) {
            System.out.print(a[i] + " ");
        }
        for (int i = n - 1; i >= n / 2; i--) {
            System.out.print(a[i] + " ");
        }
    }
}
